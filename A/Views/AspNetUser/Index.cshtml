@model List<A.Models.ApplicationUser>

@{
    ViewBag.Title = "User Management";
}
<br />
<br />
<br />
<div class="block">
    <div class="col-md-6">
        <form method="post">
            <input type="text" class="form-control" style="display:inline-block" placeholder="Search" id="name" name="name">
            <button class="btn btn-default" style="display:inline-block" type="submit">
                <i class="glyphicon glyphicon-search"></i>
            </button>
        </form>
    </div>


</div>
<br />
<br />
<br />
<table class="table table-striped">
    <tr>
        <th>Username/Email</th>
        <th>Location</th>
        <th></th>

    </tr>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].UserName</td>
            <td>@Model[i].Location</td>
            <td>

                <a href="@Url.Action("EditUser","AspNetUser", new {id = Model[i].Id.ToString()},null)" class="btn btn-default btn-md" id="editUser" target="_blank">

                    <span class="glyphicon glyphicon-pencil"></span>
                </a>
            </td>
        </tr>
    }


</table>



